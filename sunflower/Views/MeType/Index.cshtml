@model IEnumerable<BusinessLayerLibrary.MeType>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<h2>MeType Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MeTypeID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MeTypeDesc)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MeTypeID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MeTypeDesc)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MeTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MeTypeID })
        </td>
    </tr>
}

</table>
